    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch 11ty",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/node_modules/@11ty/eleventy/cmd.js",
            "args": [],
            "runtimeArgs": [
                "--preserve-symlinks"
            ]
        },
        {
            "name": "Launch Images",
            "type": "pwa-node",
            "program": "${workspaceFolder}/utils/prebuild.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
    ]
}
